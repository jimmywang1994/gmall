<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ww.gmall.pms.mapper.SkuImageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ww.gmall.pms.bean.SkuImage">
        <id column="id" property="id" />
        <result column="sku_id" property="skuId" />
        <result column="img_name" property="imgName" />
        <result column="img_url" property="imgUrl" />
        <result column="product_img_id" property="productImgId" />
        <result column="is_default" property="isDefault" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, sku_id, img_name, img_url, product_img_id, is_default
    </sql>

</mapper>
