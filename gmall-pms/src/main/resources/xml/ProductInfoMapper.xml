<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ww.gmall.pms.mapper.ProductInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ww.gmall.pms.bean.ProductInfo">
        <id column="id" property="id" />
        <result column="product_name" property="productName" />
        <result column="description" property="description" />
        <result column="catalog3_id" property="catalog3Id" />
        <result column="tm_id" property="tmId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, product_name, description, catalog3_id, tm_id
    </sql>

</mapper>
